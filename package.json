{
  "name": "mozaik-ext-travis",
  "version": "2.0.0-alpha.1",
  "description": "Mozaik travis widgets",
  "repository": {
    "type": "git",
    "url": "git://github.com/plouc/mozaik-ext-travis"
  },
  "license": "MIT",
  "author": {
    "name": "RaphaÃ«l Benitte",
    "url": "https://github.com/plouc"
  },
  "homepage": "https://github.com/plouc/mozaik-ext-travis",
  "main": "./lib/components/index.js",
  "keywords": [
    "travis",
    "ci",
    "mozaik",
    "widget",
    "extension",
    "dashboard"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "travis-ci": "^2.1.1"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "cz-conventional-changelog": "1.2.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-react-app": "^0.5.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.1.2",
    "mozaik": "^2.0.0-alpha.11",
    "nivo": "^1.0.0-alpha.8",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "peerDependencies": {
    "mozaik": "^2.0.0-alpha.11",
    "nivo": "^1.0.0-alpha.8",
    "react": "^15.4.1"
  },
  "scripts": {
    "eslint": "eslint ./src/** ./test/**",
    "test": "NODE_ENV=test ava --verbose",
    "test-cover": "NODE_ENV=test nyc ava --verbose",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "cover-report": "nyc report --reporter=lcov && open coverage/lcov-report/index.html",
    "build": "rm -rf lib/* && babel src --no-comments --out-dir lib",
    "watch": "babel src --watch --out-dir lib"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "tap": false,
    "failFast": true,
    "require": "babel-register",
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
